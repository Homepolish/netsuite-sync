<soap:Envelope xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:tns="urn:platform_2018_1.webservices.netsuite.com"
               xmlns:platformMsgs="urn:messages_2018_1.platform.webservices.netsuite.com"
               xmlns:platformCore="urn:core_2018_1.platform.webservices.netsuite.com"
               xmlns:platformFaults="urn:faults_2018_1.platform.webservices.netsuite.com">
    <soap:Header>
        <ns1:applicationInfo xmlns:ns1="urn:messages_2018_1.platform.webservices.netsuite.com">
            <applicationId>1B99D675-7E7A-4DA5-9610-04D229E10AB8</applicationId>
        </ns1:applicationInfo>
        <ns1:tokenPassport soap:actor="http://schemas.xmlsoap.org/soap/actor/next" soap:mustUnderstand="0"
