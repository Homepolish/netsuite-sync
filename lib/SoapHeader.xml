            <ns4:timestamp xmlns:ns6="urn:core_2018_1.platform.webservices.netsuite.com"><%=timestamp %></ns6:timestamp>
            <ns4:signature soap:algorithm="HMAC-SHA256" xmlns:ns7="urn:core_2018_1.platform.webservices.netsuite.com"><%=signature %></ns7:signature>
        </ns1:passport>
    </soap:Header>
    <soap:Body>
        <%=soapBody%>
